		<encodingDesc>
			<tagsDecl>
				<rendition n="mono" xml:id="mono" scheme="css">
					font-family: Courier, monospace
				</rendition>
				<!-- font-size -->
				<rendition n="italic" xml:id="i" scheme="css">
					font-style: italic;
				</rendition>
				<rendition n="normal" xml:id="n" scheme="css">
					font-style: normal;
				</rendition>
				<!-- font-variant -->
				<rendition n="small caps" xml:id="small_caps" scheme="css">
					font-variant: small-caps;
				</rendition>
				<!-- font-weight -->
				<rendition n="bold" xml:id="b" scheme="css">
					font-weight: bold;
				</rendition>
				<!-- text-decoration -->
				<rendition n="strike through" xml:id="strike" scheme="css">
					text-decoration: line-through;
				</rendition>
				<rendition n="underline" xml:id="u" scheme="css">
					text-decoration: underline;
				</rendition>
				<rendition n="double underline" xml:id="uu" scheme="css">
					text-decoration: underline; border-bottom: 1px solid black;
				</rendition>
				<rendition n="block" xml:id="block" scheme="css">
					display: block; margin: 4em;
				</rendition>
				<rendition n="blockquote" xml:id="blockquote" scheme="css">
					display: block; margin-left:4em;
				</rendition>
				<!-- text-transform -->
				<rendition n="uppercase" xml:id="upper_case" scheme="css">
					text-transform: uppercase;
				</rendition>
				<!-- text-align -->
				<rendition n="align center" xml:id="center" scheme="css">
					text-align: center;
				</rendition>
				<rendition n="align right" xml:id="right" scheme="css">
					text-align: right;
				</rendition>
				<rendition n="align left" xml:id="left" scheme="css">
					text-align: right;
				</rendition>
				<!-- block alignment: centers a block of text, but doesn't affect the alignment of text withing the centered block.-->
				<rendition n="center block" xml:id="center_block" scheme="css">
					margin-left:auto; margin-right:auto; 
				</rendition>
				<!-- text-indent: hanging indents for verse-->
				<rendition n="indent 1 level" xml:id="ti-1" scheme="css">
					text-indent: 1em;
				</rendition>
				<rendition n="indent 2 levels" xml:id="ti-2" scheme="css">
					text-indent: 2em;
				</rendition>
				<rendition n="indent 3 levels" xml:id="ti-3" scheme="css">
					text-indent: 3em;
				</rendition>
				<rendition n="indent 4 levels" xml:id="ti-4" scheme="css">
					text-indent: 4em;
				</rendition>
				<rendition n="indent 5 levels" xml:id="ti-5" scheme="css">
					text-indent: 5em;
				</rendition>
				<rendition n="indent 6 levels" xml:id="ti-6" scheme="css">
					text-indent: 6em;
				</rendition>
				<rendition n="indent 7 levels" xml:id="ti-7" scheme="css">
					text-indent: 7em;
				</rendition>
				<rendition n="indent 8 levels" xml:id="ti-8" scheme="css">
					text-indent: 8em;
				</rendition>
				<rendition n="indent 9 levels" xml:id="ti-9" scheme="css">
					text-indent: 9em;
				</rendition>
				<!-- vertical-align -->
				<rendition n="subscript" xml:id="sub" scheme="css">
					vertical-align: sub;
				</rendition>
				<rendition n="superscript" xml:id="super" scheme="css">
					vertical-align: super;
				</rendition>
				<rendition n="hanging indent" xml:id="hang" scheme="css">
					display:block;padding-left: 2em; text-indent: -2em; padding: 0;
				</rendition>
				<rendition n="inline" xml:id="inline" scheme="css">
					display: inline
				</rendition>
				<rendition n="suppress" xml:id="suppress" scheme="css">
					display: none
				</rendition>
				<rendition n="wrapped code / wrappped formatted text" xml:id="wrap">
					overflow-x: auto; /* Use horizontal scroller if needed; for Firefox 2, not needed in Firefox 3 */ white-space: pre-wrap; /* css-3 */ white-space: -moz-pre-wrap !important; /* Mozilla, since 1999 */ white-space: -pre-wrap; /* Opera 4-6 */ white-space: -o-pre-wrap; /* Opera 7 */ /* width: 99%; */ word-wrap: break-word; /* Internet Explorer 5.5+ */ 
				</rendition>
<!-- lists -->
				<rendition xml:id="list-style-type_none" n="list style: none">
					list-style-type:none;
				</rendition>
				<rendition xml:id="list-style-type_circle" n="list style: circle">
					list-style-type:circle;
				</rendition>
				<rendition xml:id="list-style-type_disc" n="list style: disc">
					list-style-type:disc;
				</rendition>
				<rendition xml:id="list-style-type_square" n="list style: square">
					list-style-type:square;
				</rendition>
				<rendition xml:id="list-style-type_armenian" n="list style: armenian">
					list-style-type:armenian;
				</rendition>
				<rendition xml:id="list-style-type_decimal" n="list style: decimal">
					list-style-type:decimal;
				</rendition>
				<rendition xml:id="list-style-type_decimal-leading-zero" n="list style: decimal-leading-zero">
					list-style-type:decimal-leading-zero;
				</rendition>
				<rendition xml:id="list-style-type_georgian" n="list style: georgian">
					list-style-type:georgian;
				</rendition>
				<rendition xml:id="list-style-type_lower-alpha" n="list style: lower-alpha">
					list-style-type:lower-alpha;
				</rendition>
				<rendition xml:id="list-style-type_lower-greek" n="list style: lower-greek">
					list-style-type:lower-greek;
				</rendition>
				<rendition xml:id="list-style-type_lower-roman" n="list style: lower-roman">
					list-style-type:lower-roman;
				</rendition>
				<rendition xml:id="list-style-type_upper-alpha" n="list style: upper-alpha">
					list-style-type:upper-alpha;
				</rendition>
				<rendition xml:id="list-style-type_upper-roman" n="list style: upper-roman">
					list-style-type:upper-roman;
				</rendition>
				<rendition xml:id="list" n="list" scheme="free">
					Indicates a element such as 
					<gi>
						castItem
					</gi>
					is rendered in list form.
				</rendition>
<!-- 
          none  	No marker
          circle 	The marker is a circle
          disc 	The marker is a filled circle. This is default
          square 	The marker is a square
          armenian 	The marker is traditional Armenian numbering
          decimal 	The marker is a number
          decimal-leading-zero 	The marker is a number padded by initial zeros (01, 02, 03, etc.)
          georgian 	The marker is traditional Georgian numbering (an, ban, gan, etc.)
          lower-alpha 	The marker is lower-alpha (a, b, c, d, e, etc.)
          lower-greek 	The marker is lower-greek (alpha, beta, gamma, etc.)
          lower-latin 	The marker is lower-latin (a, b, c, d, e, etc.)
          lower-roman 	The marker is lower-roman (i, ii, iii, iv, v, etc.)
          upper-alpha 	The marker is upper-alpha (A, B, C, D, E, etc.) 
          upper-latin 	The marker is upper-latin (A, B, C, D, E, etc.)
          upper-roman 	The marker is upper-roman (I, II, III, IV, V, etc.)
        -->
			</tagsDecl>
		</encodingDesc>
	</teiHeader>
	<text>
		<body>
			<p>
				<listBibl html:role="list">
					<bibl html:role="listItem">Test</bibl>
				</listBibl>
				<table html:role="grid">
					<head>Testing head</head>
					<row role="label" html:role="row">
						<cell role="data" html:role="columnheader" html:aria-describedby="tryme">Test data</cell>
					</row>
				</table>
				Some text here.
			</p>
		</body>
		<p html:id="tryme">If you hear me, this works.</p>
	</text>
